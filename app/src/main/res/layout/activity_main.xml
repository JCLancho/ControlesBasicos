        android:contentDescription="@string/apagada"/>

    <android.widget.SeekBar
        android:id="@+id/seekBar"
        android:layout_width="137dp"
        android:layout_height="157dp"
        android:max="100"
        android:progress="100"
        android:rotation="270"
        android:splitTrack="false"
        app:layout_constraintStart_toEndOf="@+id/imagenSol"
        app:layout_constraintTop_toTopOf="@+id/imagenSol" />

    <ImageView
        android:id="@+id/imagenSol"
        android:layout_width="156dp"
        android:layout_height="157dp"
        android:layout_marginTop="44dp"
        android:alpha="1"
        app:layout_constraintStart_toStartOf="@+id/imagenCamara1"
        app:layout_constraintTop_toBottomOf="@+id/imagenCamara1"
        app:srcCompat="@drawable/sol"
        android:contentDescription="@string/persiana"/>


</androidx.constraintlayout.widget.ConstraintLayout>