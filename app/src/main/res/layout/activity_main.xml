        android:id="@+id/botonCamara2"
        android:layout_width="53dp"
        android:layout_height="36dp"
        android:checked="true"
        android:textOff="@string/no"
        android:textOn="@string/si"
        app:layout_constraintEnd_toEndOf="@+id/switchLuz1"
        app:layout_constraintStart_toStartOf="@+id/switchLuz1"
        app:layout_constraintTop_toBottomOf="@+id/switchLuz1"
        android:onClick="toggleCamara"/>

    <ToggleButton
        android:id="@+id/botonLuz2"
        android:layout_width="53dp"
        android:layout_height="35dp"
        android:textOff="@string/no"
        android:textOn="@string/si"
        app:layout_constraintStart_toStartOf="@+id/botonCamara2"
        app:layout_constraintTop_toBottomOf="@+id/botonCamara2"
        android:onClick="toggleLuz" />

    <ImageView
        android:id="@+id/imagenCamara1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="28dp"
        app:layout_constraintStart_toStartOf="@+id/luz2"
        app:layout_constraintTop_toBottomOf="@+id/luz2"
        app:srcCompat="@drawable/camara"
        android:contentDescription="@string/camara"/>

    <ImageView
        android:id="@+id/imagenLuz1"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/imagenCamara1"
        app:layout_constraintTop_toTopOf="@+id/imagenCamara1"
        app:srcCompat="@drawable/bombilla_apagada"
        android:contentDescription="@string/apagada"/>

    <ImageView
        android:id="@+id/imagenCamara2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toEndOf="@+id/imagenLuz1"
        app:layout_constraintTop_toTopOf="@+id/imagenLuz1"
        app:srcCompat="@drawable/camara"
        android:contentDescription="@string/camara"/>

    <ImageView
        android:id="@+id/imagenLuz2"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
