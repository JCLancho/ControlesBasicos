        app:popupTheme="@style/AppTheme.PopupOverlay"
        app:title="Domotica"
        tools:layout_editor_absoluteX="-16dp" />

    <TextView
        android:id="@+id/camara1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/camara1"
        app:layout_constraintStart_toStartOf="@+id/toolbar"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/luz1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/luz1"
        app:layout_constraintStart_toStartOf="@+id/camara1"
        app:layout_constraintTop_toBottomOf="@+id/camara1" />

    <TextView
        android:id="@+id/camara2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/camara2"
        app:layout_constraintStart_toStartOf="@+id/luz1"
        app:layout_constraintTop_toBottomOf="@+id/luz1" />

    <TextView
        android:id="@+id/luz2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/luz2"
        app:layout_constraintStart_toStartOf="@+id/camara2"
        app:layout_constraintTop_toBottomOf="@+id/camara2" />

    <Switch
        android:id="@+id/switchCamara1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginTop="28dp"
        android:checked="true"
        app:layout_constraintStart_toStartOf="@+id/toolbar"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        android:onClick="toggleCamara"/>

    <Switch
        android:id="@+id/switchLuz1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
